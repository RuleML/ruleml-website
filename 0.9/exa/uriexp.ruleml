<?xml version="1.0" encoding="UTF-8"?>

<RuleML
xmlns="http://www.ruleml.org/0.9/xsd"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.ruleml.org/0.9/xsd
http://www.ruleml.org/0.9/xsd/hornlogeq.xsd">

<!-- a uri expander -->

<Assert mapClosure="universal">

  <Equal>
    <Nano>
      <op>
        <Fun>uriexp</Fun>
      </op>
      <Ind>daml</Ind>
    </Nano>
    <Ind uri="http://www.daml.org/"/>
  </Equal>

  <Equal>
    <Nano>
      <op>
        <Fun>uriexp</Fun>
      </op>
      <Ind>oil</Ind>
    </Nano>
    <Ind uri="http://www.ontoknowledge.org/oil/"/>
  </Equal>

  <Equal>
    <Nano>
      <op>
        <Fun>uriexp</Fun>
      </op>
      <Ind>w3c</Ind>
    </Nano>
    <Ind uri="http://www.w3.org/"/>
  </Equal>


</Assert>

</RuleML>